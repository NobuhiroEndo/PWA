    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "run-p -c lint:*",
    "lint:scripts": "eslint --ext .js,.jsx ./",
    "lint:styles": "stylelint \"src/**/*.css\" --config .stylelintrc.js",
    "format": "run-p -c format:*",
